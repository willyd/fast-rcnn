<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" ToolsVersion="4.0">
  <PropertyGroup>
    <Configuration Condition=" '$(Configuration)' == '' ">Debug</Configuration>
    <SchemaVersion>2.0</SchemaVersion>
    <ProjectGuid>2b9fd2da-d0a8-4996-8a1b-be03ef7164c3</ProjectGuid>
    <ProjectHome>.</ProjectHome>
    <StartupFile>..\..\..\tools\demo.py</StartupFile>
    <SearchPath>
    </SearchPath>
    <WorkingDirectory>.</WorkingDirectory>
    <OutputPath>.</OutputPath>
    <Name>fast-rcnn</Name>
    <RootNamespace>fast-rcnn</RootNamespace>
    <LaunchProvider>Standard Python launcher</LaunchProvider>
    <InterpreterArguments>
    </InterpreterArguments>
    <EnableNativeCodeDebugging>False</EnableNativeCodeDebugging>
    <CommandLineArguments>--gpu 0 --net caffenet</CommandLineArguments>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
    <DebugSymbols>true</DebugSymbols>
    <EnableUnmanagedDebugging>false</EnableUnmanagedDebugging>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <DebugSymbols>true</DebugSymbols>
    <EnableUnmanagedDebugging>false</EnableUnmanagedDebugging>
  </PropertyGroup>
  <PropertyGroup>
    <VisualStudioVersion Condition="'$(VisualStudioVersion)' == ''">10.0</VisualStudioVersion>
    <PtvsTargetsFile>$(MSBuildExtensionsPath32)\Microsoft\VisualStudio\v$(VisualStudioVersion)\Python Tools\Microsoft.PythonTools.targets</PtvsTargetsFile>
  </PropertyGroup>
  <ItemGroup>
    <Content Include="..\..\..\LICENSE">
      <Link>LICENSE</Link>
    </Content>
    <Content Include="..\..\..\README.md">
      <Link>README.md</Link>
    </Content>
    <Content Include="..\..\..\**\*.py;..\..\..\**\*.pyx" Exclude="..\..\..\ptvs">
      <Link>%(RecursiveDir)%(Filename)%(Extension)</Link>
    </Content>
  </ItemGroup>
  <ItemGroup>
    <Compile Include="copy_pycaffe.py">
      <SubType>Code</SubType>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <Folder Include="caffe-fast-rcnn\" />
    <Folder Include="caffe-fast-rcnn\python\" />
    <Folder Include="caffe-fast-rcnn\python\caffe\" />
    <Folder Include="caffe-fast-rcnn\python\caffe\proto\" />
    <Folder Include="caffe-fast-rcnn\python\caffe\test\" />
    <Folder Include="lib\" />
    <Folder Include="lib\datasets\" />
    <Folder Include="lib\fast_rcnn\" />
    <Folder Include="lib\roi_data_layer\" />
    <Folder Include="lib\utils\" />
    <Folder Include="ptvs\" />
    <Folder Include="ptvs\fast-rcnn\" />
    <Folder Include="ptvs\fast-rcnn\faster-rcnn\" />
    <Folder Include="tools\" />
  </ItemGroup>
  <Import Condition="Exists($(PtvsTargetsFile))" Project="$(PtvsTargetsFile)" />
  <Import Condition="!Exists($(PtvsTargetsFile))" Project="$(MSBuildToolsPath)\Microsoft.Common.targets" />
  <!-- Uncomment the CoreCompile target to enable the Build command in
       Visual Studio and specify your pre- and post-build commands in
       the BeforeBuild and AfterBuild targets below. -->
  <Target Name="CoreCompile">
    <Message Text="Building fast-rcnn cython files" Importance="high" />
    <Exec Command="python setup.py build_ext --inplace" WorkingDirectory="..\..\..\lib" />
    <Exec Command="rmdir /S /Q build" WorkingDirectory="..\..\..\lib" />
    <Message Text="Copying pycaffe" Importance="high" />
    <Exec Command="python .\copy_pycaffe.py" />
  </Target>
  <Target Name="BeforeBuild">
  </Target>
  <Target Name="AfterBuild">
  </Target>
</Project>